---
layout: null
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Mailplane Blog</title>
  <link href="http://www.mailplaneapp.com/blog.html"/>
  <link type="application/atom+xml" rel="self" href="http://www.mailplaneapp.com/blog/rss_2.0"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://www.mailplaneapp.com/blog</id>
  <author>
    <name>Mailplane Team</name>
    <email>support@mailplaneapp.com</email>
  </author>

  {% for post in site.categories.blog %}
  <entry>
    <id>http://www.mailplaneapp.com/blog{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://www.mailplaneapp.com/{{ post.url }}"/>
    <title>{{ post.title | xml_escape }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>{{ post.author }}</name>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>